{
  "name": "electron-react-starter",
  "description": "A starter app for electron + react.",
  "author": {
    "name": "kevin-lin",
    "email": "kevin.lin@coxautoinc.com"
  },
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.0",
    "typescript": "^3.4.5"
  },
  "homepage": "./",
  "main": "public/electron.js",
  "scripts": {
    "clean": "rimraf build/**/* dist/**/*",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"yarn start\" \"wait-on http://localhost:3000 && electron .\" --kill-others",
    "electron-pack": "yarn clean && yarn build && node build.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^7.2.4",
    "electron-builder": "^20.39.0",
    "rimraf": "^2.6.3",
    "wait-on": "^3.2.0"
  }
}
